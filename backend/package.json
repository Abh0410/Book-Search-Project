{
  "name": "book-search-backend",
  "version": "1.0.0",
  "description": "Backend API for Book Search Application with Elasticsearch",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup-elasticsearch": "node scripts/setup-elasticsearch.js",
    "seed-data": "node scripts/seed-data.js",
    "debug": "node --inspect server.js",
